            android:label="Karya Bahasa Dan Sastra"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".KaryaKomunitas"
            android:label="Karya Komunitas"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".KaryaGuru"
            android:label="Karya Guru"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Angkasa"
            android:label="Wahana Angkasa"
