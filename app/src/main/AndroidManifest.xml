        <activity
            android:name=".KelasDigital"
            android:label="Kelas Digital"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".BankSoal"
            android:label="Bank Soal"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".LabMaya"
            android:label="Lab Maya"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SumberBelajar"
            android:label="Sumber Belajar"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".activitySplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">

        </activity>
    </application>

</manifest>